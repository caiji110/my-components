.contact-likeyou{
  width: 280rpx;
  height: 224rpx;
  background: #adadad;
  margin: 162rpx auto 80rpx auto;
}

.right-title{
  width: 290rpx;
  font-family: "PingFang SC";
font-weight: bold;
font-size: 36rpx;
text-align: left;
color: #12181f;
margin: 0 auto;

}
.right-num{
  width: 290rpx;
  margin:0 auto;
  margin-top:16rpx;
  text-align: center;
  font-family: "PingFang SC";
font-weight: bold;
font-size: 50rpx;
color: #f15757;
transform: translate(-25rpx,0);
}
.answer-state-false,.answer-state-true{
  font-family: "PingFang SC";
font-weight: bold;
font-size: 32rpx;
text-align: center;
color: #f15757;
min-width: 504rpx;
margin: 96rpx 0 0 auto;
}
.answer-state-false>view,.answer-state-true>view{
  margin-bottom: 10rpx;
}
.built-game-btn{
  margin: 0 auto;
  margin-top: 264rpx;
  width: 224rpx;
  height: 62rpx;
  line-height: 63rpx;
  border-radius: 10rpx;
  background: #9573e7;
  color: white;
  font-size: 32rpx;
  font-weight: normal;
}
.sorry{
  transform: translate(-25rpx,0);
}
